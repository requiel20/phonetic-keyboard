    <!--AppBar-->
    <Page.BottomAppBar>
        <AppBar>
            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal" Grid.Column="1">
                <Button x:ConnectionId='1' x:Name="btShowLab" x:Uid="btShowLab"                          Content="Nascondi le etichete"/>
                <Button x:ConnectionId='2' x:Name="btShowBt" x:Uid="btShowBt"                         Content="Nascondi tutti i tasti" Margin="20,0,10,0"/>
                <Rectangle Height="50" Width="2" Fill="LightGray" Margin="20,0,20,0"/>
                <CheckBox x:ConnectionId='3' x:Name="cbPlay" x:Uid="cbPlay"  FontSize="18"  Margin="0,0,0,0" IsChecked="True"                                                       >Ascolta il fono alla pressione di ogni tasto</CheckBox>
                <Rectangle Height="50" Width="2" Fill="LightGray" Margin="20,0,20,0"/>
                <Button x:ConnectionId='4' x:Name="btPlay" Style="{StaticResource PlayAppBarButtonStyle}"                      />
                <Button x:ConnectionId='5' x:Name="btStop" Style="{StaticResource StopAppBarButtonStyle}"                     />
            </StackPanel>
        </AppBar>
    </Page.BottomAppBar>

